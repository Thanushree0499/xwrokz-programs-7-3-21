<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>RosePetalsBotique</display-name>
  <welcome-file-list>
    <welcome-file>register.html</welcome-file>

  </welcome-file-list>
  
  <servlet>
  <servlet-name>registerPageservlet</servlet-name>
  <servlet-class>com.xworkz.botique.RegistrationPage</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>registerPageservlet</servlet-name>
  <url-pattern>/registerpageurl</url-pattern>
  </servlet-mapping>
  
    <servlet>
  <servlet-name>loginpageservlet</servlet-name>
  <servlet-class>com.xworkz.botique.LoginPage</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>loginpageservlet</servlet-name>
  <url-pattern>/loginpageurl</url-pattern>
  </servlet-mapping>
  
 
 
</web-app>